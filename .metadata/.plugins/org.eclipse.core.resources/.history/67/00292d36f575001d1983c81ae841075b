package com.kani.ram.application2;

public class BankRBI {
	
	 static {
		 System.out.println("Welcome to Employee Details");
	 }
	 public BankRBI() {
		 System.out.println("RBI Bank Home Loan and Personal Loan Interest rates..");
	}
	 
	 public BankRBI(double p, double r) {
		  System.out.println(p*r/100);
	}
	 
	 public static final void getBankDetails() {
		 System.out.println("Home Loan Interest In 7.5%");
		 System.out.println("Personal  Loan Interest In 11%");
	 }
}
