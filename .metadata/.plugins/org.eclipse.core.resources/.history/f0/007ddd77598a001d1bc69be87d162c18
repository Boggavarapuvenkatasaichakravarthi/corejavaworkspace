package com.kani.CorejavaGeneral;

import java.util.Scanner;

public class FactrioalNo {

	public static void main(String[] args) {
		int n=1,fact=1,t=1;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the FactrioalNumber:");
		t=sc.nextInt();
		while(n<=t)
		{
			fact=fact*n;
			n++;
			
		}
		System.out.println(" The Factrioal Number:"+fact);
		

	}

}
